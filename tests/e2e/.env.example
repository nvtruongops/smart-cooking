# Smart Cooking E2E Test Environment Configuration
# Copy this file to .env and fill in your actual values

# Test Environment (dev/prod)
TEST_ENV=dev

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here

# Development Environment
DEV_API_URL=https://api-dev.smartcooking.com/v1
DEV_USER_POOL_ID=us-east-1_devpool123
DEV_USER_POOL_CLIENT_ID=devclient123
DEV_TABLE_NAME=smart-cooking-data-dev

# Production Environment
PROD_API_URL=https://api.smartcooking.com/v1
PROD_USER_POOL_ID=us-east-1_prodpool123
PROD_USER_POOL_CLIENT_ID=prodclient123
PROD_TABLE_NAME=smart-cooking-data-prod

# Test Configuration
DEBUG=false
TEST_TIMEOUT=120000

# Optional: Override specific endpoints for testing
# CUSTOM_API_URL=http://localhost:3000/api
# CUSTOM_USER_POOL_ID=local-pool-id