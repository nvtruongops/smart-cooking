{
  "name": "cooking-session-lambda",
  "version": "1.0.0",
  "description": "Lambda function for cooking session management",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>"],
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": [
      "*.ts",
      "!*.test.ts",
      "!*.d.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!uuid)"
    ],
    "moduleNameMapper": {
      "^uuid$": "uuid"
    }
  }
}